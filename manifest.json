{
  "manifest_version": 2,
  "name": "Website Automation",
  "version": "0.1",
  "permissions": [
    "activeTab"
  ],
  "icons": {
		"48": "images/icon_48.png",
		"128": "images/icon_128.png"
	},
  "browser_action":{
    "default_icon":{
      "19": "images/icon_19.png",
			"38": "images/icon_38.png"
    },
    "default_popup":"popup.html",
    "defult_title":"website automation"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ]
}
