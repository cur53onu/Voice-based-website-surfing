  {
  "manifest_version": 2,
  "name": "Website Automation",
  "version": "0.1",
  "permissions": [
    "activeTab"
  ],
  "browser_action":{
    "default_icon":"images/traversy-logo.png",
    "default_popup":"popup.html",
    "defult_title":"website automation"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ]
}
